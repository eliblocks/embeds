<div class="test-class"></div>
<div class="minutes-balance">
  <%= link_to root_path, class: "icon-link" do %>
    <div class="link-container">
      <%= image_tag "browzable_logo_white.png", width: 130, height: 32 %> 
      <span class="divider-player">&#8226;</span>
      <span id="balance" data-balance="<%= current_account.balance %>">
        <%= number_to_human(current_account.balance / 60) %>
      </span>
    </div>
  <% end %>
</div>
<div class="test-class"></div>
<%= content_tag :div, "", class: "browzable-wrapper", data: { url: @video.cloudfront_url, video_id: @video.id } do %>
  <div id="browzable-video"></div>
<% end %>
